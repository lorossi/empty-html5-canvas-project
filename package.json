{
  "name": "empty-html5-canvas-project",
  "version": "1.0.0",
  "description": "Crude html5 canvas project template.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "test": "c8 mocha --require ./test/setup-dom.mjs 'test/**/*.mjs'",
    "docs": "jsdoc js/*.js -d docs/",
    "bundle": "rollup -c",
    "build": "mkdir -p dist/js && npm run bundle && cp index.html dist/ && cp -r css/ dist/ && cp js/sketch.js dist/js/sketch.js && cp js/inject.js dist/js/inject.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorossi/empty-html5-canvas-project.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/lorossi/empty-html5-canvas-project/issues"
  },
  "homepage": "https://github.com/lorossi/empty-html5-canvas-project#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.3",
    "canvas": "^3.2.0",
    "chai": "^6.2.2",
    "jsdoc": "^4.0.5",
    "jsdom": "^27.4.0",
    "mocha": "^11.7.5",
    "rollup": "^4.54.0",
    "terser": "^5.44.1"
  }
}